var downloads = document.querySelector("#user-content");

downloads.dataset.contentType = "user-images-downloaded"

downloads.removeChild(downloads.firstElementChild)
downloads.insertAdjacentHTML("beforeend", "<%= j( render :partial => 'downloads_card' ) %>");


var no_content_tag = document.querySelector("#no-content");

if (no_content_tag)
  no_content_tag.textContent = "You haven't downloaded any images yet :("
