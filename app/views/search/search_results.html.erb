<div class="search_results">
  <% unless @search_results.empty? %>
    <% @search_results.each do |img| %>
      <div class="<%= img["title"] %>">
        <span><%= img["title"] %></span>
      </div>
      <div class="<%= img["title"] %>">
        <span><%= img["location"] %></span>
      </div>
    <% end %>
  <% else %>
    <span>No images were found</span>
  <% end %>
</div>
